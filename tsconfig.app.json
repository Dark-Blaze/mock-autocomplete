{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "src/*": [
        "src/*"
      ],
      "components/*": [
        "src/app/components/*"
      ],
      "pages/*": [
        "app/pages/*"
      ],
      "utils/*": [
        "src/app/utils/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
}
