{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
  "paths": {
    "src/*": ["src/*"],
    "components/*": ["src/app/components/*"],
    "pages/*": ["app/pages/*"],
    "utils/*": ["src/app/utils/*"]
},
}
